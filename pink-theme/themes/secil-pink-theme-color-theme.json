		"statusBarItem.remoteBackground": "#ff99e2",
		"statusBarItem.remoteForeground": "#ffffff",
		"titleBar.activeBackground": "#141828",
		"pickerGroup.border": "#FFFFFF1a",
		"debugToolBar.background": "#242b47",
		"selection.background": "#ff99e2"
	},
	
}